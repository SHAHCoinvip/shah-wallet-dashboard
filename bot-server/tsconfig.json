{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "outDir": "dist",
    "rootDir": ".",
    "esModuleInterop": true,
    "strict": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "moduleResolution": "node"
  },
  "include": ["bot.ts", "src/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
